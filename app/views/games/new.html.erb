<h2> Game of Thrones Form</h2>



<%= form_tag ('/games', method: 'post') do %>

<div>
  <%= label_tag(:character) %>
  <%= text_field_tag(:character) %>
</div>

<div>
  <%= label_tag(:house) %>
  <%= text_field_tag(:house) %>
</div>


<div>
  <%= label_tag(:city) %>
  <%= text_field_tag(:city) %>
</div>

<div>
  <%= submit_tag("Create Character") %>
</div>

<% end %>
